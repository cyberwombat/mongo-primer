{
  "name": "mongo-primer",
  "version": "0.0.1",
  "description": "A simple E6S fixture loader for Mongo baed testing",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "lodash": "^4.14.1",
    "mongodb": "^2.1.7"
  },
  "devDependencies": {
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "mocha": "^3.0.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register test/index.spec.js",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "keywords": [
    "mongo",
    "mongoose",
    "fixtures",
    "mocha",
    "testing",
    "unit",
    "e2e"
  ],
  "author": "Aruga Sidamo",
  "license": "ISC",
  "homepage": "https://github.com/cyberwombat/mongo-primer",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyberwombat/mongo-primer.git"
  },
  "bugs": {
    "url": "https://github.com/cyberwombat/mongo-primer/issues"
  }
}