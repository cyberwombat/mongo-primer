{
  "name": "mongo-primer",
  "version": "0.2.1",
  "description": "A simple fixture loader for Mongo based testing",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "optionalDependencies": {
    "mongodb": "^2.2.9"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register test/index.spec.js",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "keywords": [
    "mongo",
    "mongoose",
    "fixtures",
    "mocha",
    "testing",
    "unit",
    "e2e"
  ],
  "author": "cyberwombat",
  "license": "ISC",
  "homepage": "https://github.com/cyberwombat/mongo-primer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyberwombat/mongo-primer.git"
  },
  "bugs": {
    "url": "https://github.com/cyberwombat/mongo-primer/issues"
  }
}
